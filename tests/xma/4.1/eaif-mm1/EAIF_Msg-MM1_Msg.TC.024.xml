<test>
    <configuration>../xmaConfig.xml</configuration>
    <name>EAIF_Msg-MM1_Msg.TC.024</name>
    <description>In the incoming MM Message Neither X-NOKIA-MMSC-From HTTP Header nor From Header of m-send-req PDU are present [X-Mms-Delivery-Report=Yes, X-Mms-Read-Report=Yes]</description>

    <!-- Delete Message Store -->
    <subtest>../deleteMessagestoreAll.xml</subtest>
    
    <!-- Load VASP File -->
    <subtest>../loadVASP.xml</subtest>

    <function name="setVariable" module="Basic">
        <parameter>ADDR_TO</parameter>
       <configuration>XMA/POSTPAY/ENABLED/MSISDN2</configuration>
    </function> 
   
    <function name="setVariable" module="Basic">
        <parameter>ADDR_FROM</parameter>
        <configuration>EAIF/NUMBER</configuration>
    </function>

    <function name="startSMSCServer" module="SMS"/>
    
    <function name="waitForBind" module="SMS"/>

    <function name="createMM1Packet" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>M_SEND_REQ</parameter>
        <parameter>XTTTRANSACTIONID0001</parameter>
        <parameter>0x17</parameter>
        <variable>ADDR_TO</variable>
        <parameter></parameter>
        <parameter>text/plain</parameter>
        <parameter>utf-8</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>X_MMS_DELIVERY_REPORT</parameter>
        <parameter>false</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>X_MMS_READ_REPORT</parameter>
        <parameter>false</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>SUBJECT</parameter>
        <parameter>No, they're all going to laugh at you!</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>X_MMS_MESSAGE_CLASS</parameter>
        <parameter>128</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>X_MMS_PRIORITY</parameter>
        <parameter>130</parameter>
    </function>

    <function name="createMM1PacketOptions" module="SMS">
        <parameter>sendPacket1</parameter>
        <parameter>X_MMS_SENDER_VISIBILITY</parameter>
        <parameter>129</parameter>
    </function>
   
    <function name="createMM1PacketContent" module="SMS">
        <parameter>sendPacket1</parameter>
        <base64encode>Hello World!</base64encode>
    </function>

    <function name="storeMM1Packet" module="SMS">
        <parameter>sendPacket1Base64</parameter>
        <parameter>sendPacket1</parameter>
    </function>

    <function name="decodeMM1Packet" module="SMS">
        <parameter>PACKET1Request</parameter>
        <variable>sendPacket1Base64</variable>
    </function>
     
    <function name="setHeader" module="HTTP">
        <parameter>Content-Type</parameter>
        <parameter>application/vnd.wap.mms-message</parameter>
    </function>

	<function name="setBase64PostData" module="HTTP">
        <variable>sendPacket1Base64</variable>
    </function>
    
     <function name="sendPostRequest" module="HTTP">
        <parameter>
            <parameter>http://</parameter>
            <configuration>XMA/EAIFIP</configuration>
            <parameter>:</parameter>
            <configuration>XMA/EAIFPORT</configuration>
            <parameter>/</parameter>
        </parameter>
    </function>

    <function name="checkResponseCode" module="HTTP">
        <parameter>499</parameter>
    </function>

	<!-- Restore VASP File -->
    <subtest>../restoreVASP.xml</subtest>
  
    <function name="stopSMSCServer" module="SMS"/>

</test>
