<test>

	<name>MM1_Msg-EAIF_Msg.TC.042</name>
	<description>10 Recipients distributed across To, Cc and Bcc headers [X-Mms-Delivery-Report=Yes, X-Mms-Read-Report=Yes] [Sender's DB_Prepaid=false]</description>

	<configuration>../xmaConfig.xml</configuration>

	<!-- Delete Message Store -->
	<subtest>../deleteMessagestoreAll.xml</subtest>

	<!-- Load VASP File -->
	<subtest>../loadMultiEaif.xml</subtest>

	<!-- Set EAIF Address - Request is being hit from -->
	<function name="setVariable" module="Basic">
		<parameter>ADDR_FROM</parameter>
		<configuration>XMA/POSTPAY/ENABLED/MSISDN1</configuration>
	</function>

	<!-- Start Web Server -->
	<function name="startWebServer" module="HTTP">
		<configuration>EAIF/PORT</configuration>
	</function>

	<!-- Start SMSC Server -->
	<function name="startSMSCServer" module="SMS"/>
	<function name="waitForBind" module="SMS"/>

	<function name="createMM1Packet" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>M_SEND_REQ</parameter>
		<parameter>XTTTRANSACTIONID0001</parameter>
		<parameter>0x17</parameter>
		<parameter></parameter>
		<variable>ADDR_FROM</variable>
		<parameter>text/plain</parameter>
		<parameter>utf-8</parameter>
	</function>

	<function name="setVariable" module="BASIC">
		<parameter>devider</parameter>
		<parameter></parameter>
	</function>

	<!-- Setting loop for TO recipients -->
	<function name="setVariable" module="BASIC">
		<parameter>to_list</parameter>
		<parameter></parameter>
	</function>

	<loop name="loop" start="0" stop="4" step="1">
		<function name="createMM1PacketOptions" module="SMS">
			<parameter>sendPacket1</parameter>
			<parameter>TO</parameter>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>to_list</parameter>
			<parameter>
			<variable>to_list</variable>
			<variable>devider</variable>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
			</parameter>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>devider</parameter>
			<parameter>;</parameter>
		</function>
	</loop>

	<!-- Setting loop for CC recipients -->
	<function name="setVariable" module="BASIC">
		<parameter>devider</parameter>
		<parameter></parameter>
	</function>

	<function name="setVariable" module="BASIC">
		<parameter>cc_list</parameter>
		<parameter></parameter>
	</function>

	<loop name="loop" start="4" stop="7" step="1">
		<function name="createMM1PacketOptions" module="SMS">
			<parameter>sendPacket1</parameter>
			<parameter>CC</parameter>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>cc_list</parameter>
			<parameter>
			<variable>cc_list</variable>
			<variable>devider</variable>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
			</parameter>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>devider</parameter>
			<parameter>;</parameter>
		</function>
	</loop>

	<!-- Setting loop for BCC recipients -->
	<function name="setVariable" module="BASIC">
		<parameter>devider</parameter>
		<parameter></parameter>
	</function>

	<function name="setVariable" module="BASIC">
		<parameter>bcc_list</parameter>
		<parameter></parameter>
	</function>

	<loop name="loop" start="7" stop="10" step="1">
		<function name="createMM1PacketOptions" module="SMS">
			<parameter>sendPacket1</parameter>
			<parameter>BCC</parameter>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>bcc_list</parameter>
			<parameter>
			<variable>bcc_list</variable>
			<variable>devider</variable>
			<configuration>
			<parameter>EAIF/NUMBER</parameter>
			<variable>loop</variable>
			</configuration>
			</parameter>
		</function>

		<function name="setVariable" module="BASIC">
			<parameter>devider</parameter>
			<parameter>;</parameter>
		</function>
	</loop>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>X_MMS_DELIVERY_REPORT</parameter>
		<parameter>true</parameter>
	</function>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>X_MMS_READ_REPORT</parameter>
		<parameter>true</parameter>
	</function>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>SUBJECT</parameter>
		<parameter>No, they're all going to laugh at you!</parameter>
	</function>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>X_MMS_MESSAGE_CLASS</parameter>
		<parameter>128</parameter>
	</function>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>X_MMS_PRIORITY</parameter>
		<parameter>130</parameter>
	</function>

	<function name="createMM1PacketOptions" module="SMS">
		<parameter>sendPacket1</parameter>
		<parameter>X_MMS_SENDER_VISIBILITY</parameter>
		<parameter>129</parameter>
	</function>

	<function name="createMM1PacketContent" module="SMS">
		<parameter>sendPacket1</parameter>
		<base64encode>Hello World!</base64encode>
	</function>

	<function name="storeMM1Packet" module="SMS">
		<parameter>sendPacket1Base64</parameter>
		<parameter>sendPacket1</parameter>
	</function>

	<function name="decodeMM1Packet" module="SMS">
		<parameter>PACKET1Request</parameter>
		<variable>sendPacket1Base64</variable>
	</function>

	<function name="setHeader" module="HTTP">
		<configuration>SMSCSERVER/MM1MSISDNHEADER</configuration>
		<variable>ADDR_FROM</variable>
	</function>

	<function name="setHeader" module="HTTP">
		<parameter>Accept</parameter>
		<parameter>text/vnd.wap.wml, application/xml, image/gif, image/jpeg, application/vnd.wap.mms-message</parameter>
	</function>

	<function name="setHeader" module="HTTP">
		<parameter>Content-Type</parameter>
		<parameter>application/vnd.wap.mms-message</parameter>
	</function>

	<!-- Set Return Code/Message for Web Server -->
	<function name="setServerReturnCode" module="HTTP">
		<parameter>204</parameter>
	</function>

	<function name="setServerReturnMessage" module="HTTP">
		<parameter> No Content</parameter>
	</function>

	<!-- Set Return Headers for Web Server -->
	<function name="setServerHeader" module="HTTP">
		<parameter>X-NOKIA-MMSC-From</parameter>
		<variable>ADDR_FROM</variable>
	</function>

	<function name="setServerHeader" module="HTTP">
		<parameter>X-NOKIA-MMSC-Version</parameter>
		<parameter>1.7</parameter>
	</function>

	<function name="setBase64PostData" module="HTTP">
		<variable>sendPacket1Base64</variable>
	</function>

	<function name="sendPostRequest" module="HTTP">
		<parameter>
			<parameter>http://</parameter>
			<configuration>SMSCSERVER/MM1Ip</configuration>
			<parameter>:</parameter>
			<configuration>SMSCSERVER/MM1Port</configuration>
			<parameter>/</parameter>
		</parameter>
	</function>

	<function name="checkResponseCode" module="HTTP">
		<parameter>200</parameter>
	</function>

	<function name="decodeMM1Packet" module="SMS">
		<parameter>PACKET1Response</parameter>
		<variable>HTTP/BODY/BASE64</variable>
	</function>

	<function name="queryVariable" module="BASIC">
		<parameter>PACKET1Response/MMS/x_mms_response_status</parameter>
		<parameter>dev/null</parameter>
		<parameter>Ok</parameter>
	</function>

	<function name="setVariable" module="BASIC">
		<parameter>originalMessageId</parameter>
		<variable>PACKET1Response/MMS/message_id</variable>
	</function>

	<function name="setVariable" module="BASIC">
		<parameter>log/messageid</parameter>
		<variable>originalMessageId</variable>
	</function>

	<!-- Wait for 10 EAIF messages on WebServer -->
	<function name="waitForPOSTs" module="HTTP">
		<parameter>10</parameter>
	</function>

	<!-- Wait for 10 SMSC messages on WebServer -->
	<function name="waitForWSPMessages" module="SMS">
		<parameter>10</parameter>
	</function>

	<loop name="loop" start="0" stop="10" step="1">
		<function name="addVariable" module="Basic">
			<parameter>messages</parameter>
			<parameter>1</parameter>
			<variable>loop</variable>
		</function>

		<function name="waitForWSPMessages" module="SMS">
			<variable>messages</variable>
		</function>

		<function name="compareString" module="BASIC">
			<variable>originalMessageId</variable>
			<variable>
			<parameter>SMSC/</parameter>
			<configuration>SMSCSERVER/PROTOCOL</configuration>
			<parameter>/</parameter>
			<variable>ADDR_FROM</variable>
			<parameter>/MMS/message_id</parameter>
			</variable>
		</function>

		<function name="compareString" module="BASIC">
			<parameter>Forwarded</parameter>
			<variable>
			<parameter>SMSC/</parameter>
			<configuration>SMSCSERVER/PROTOCOL</configuration>
			<parameter>/</parameter>
			<variable>ADDR_FROM</variable>
			<parameter>/MMS/x_mms_status</parameter>
			</variable>
		</function>
	</loop>

	<!-- Stop WebServer -->
	<function name="stopWebServer" module="HTTP"/>

	<!-- Stop SMSC Server -->
	<function name="stopSMSCServer" module="SMS"/>

	<!-- Remove VASP File -->
	<subtest>../restoreVASP.xml</subtest>

</test>