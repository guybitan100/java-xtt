        <parameter>Accept</parameter>
        <parameter>text/vnd.wap.wml, application/xml, image/gif, image/jpeg, application/vnd.wap.mms-message</parameter>
    </function-->

	<function name="setHeader" module="HTTP">
        <configuration>SMSCSERVER/MM1MSISDNHEADER</configuration>
        <variable>ADDR_TO</variable>
    </function>

    <function name="sendGetRequest" module="HTTP">
        <variablepointer>x_mms_content_location</variablepointer>
    </function>

	<function name="decodeMM1Packet" module="SMS">
        <parameter>PACKET1Get</parameter>
        <variable>HTTP/BODY/BASE64</variable>
    </function>
   
    <function name="checkResponseCode" module="SMS">
        <parameter>200</parameter>
    </function>

	 <function name="waitForRequests" module="STI">
        <parameter>1</parameter>
    </function>

	 <function name="validateXML" module="XML">
        <variable>STISERVER/STI/SOAP</variable>
        <parameter></parameter>
        <parameter>http://schemas.xmlsoap.org/soap/envelope/</parameter>
        <parameter>soapEnvelope.xsd</parameter>
        <parameter>http://www.openmobilealliance.org/schema/sti/v1_0</parameter>
        <parameter>OMA-TS-SCHEMA-STI-V1_0-20050607-C.xsd</parameter>
    </function>

    <function name="sendMM1AcknowledgeIndicator" module="SMS">
        <parameter>yes</parameter>
        <variablepointer>x_mms_transaction_id</variablepointer>
        <variable>ADDR_TO</variable>
    </function>

    <function name="checkResponseCode" module="SMS">
        <parameter>200</parameter>
    </function>

   <function name="waitForPOSTs" module="HTTP">
        <parameter>1</parameter>
    </function>

    <function name="setVariable" module="BASIC">
        <parameter>MessageID</parameter>
        <variable>HTTP/HEADER/x-nokia-mmsc-message-id</variable>
    </function>

	<function name="activeProcessToVar" module="Remote">
        <parameter>MSP</parameter>
        <parameter>msp</parameter>
    </function>

    <function name="executeRemoteXMSCommand" module="Remote">
        <parameter>
            <parameter>run command amocli -k -e (MMS_Req_Message-ID=='</parameter>
            <variable>MessageID</variable>
            <parameter>') search TI:F.CEE.</parameter>
            <variable>msp</variable>
            <parameter>:AMO</parameter>
        </parameter>
    </function>

    <function name="queryResponse" module="Remote">
        <parameter>MMS_Req_X-Mms-Message-Type=m-delivery-ind</parameter>
        <parameter>status</parameter>
    </function>

	<function name="queryResponse" module="Remote">
        <parameter>MMS_Req_X-Mms-Status=Retrieved</parameter>
        <parameter>status</parameter>
    </function>

	<function name="queryResponse" module="Remote">
        <parameter>MMS_DELIVERY_Status=Delivered</parameter>
        <parameter>status</parameter>
    </function>

	<function name="sendMM1ReadRec" module="SMS">
        <parameter>read</parameter>
       <variable>
            <parameter>PACKET1Get/MMS/message_id</parameter>
        </variable>
        <variable>ADDR_FROM</variable>
        <variable>ADDR_TO</variable>
        <variable>ADDR_TO</variable>
    </function>

	<function name="checkResponseCode" module="SMS">
        <parameter>200</parameter>
    </function>
    
    <!-- Wait for Read Reports -->
	<function name="waitForPOSTs" module="HTTP">
        <parameter>1</parameter>
    </function>

  <function name="setVariable" module="BASIC">
        <parameter>MessageID</parameter>
        <variable>HTTP/HEADER/x-nokia-mmsc-message-id</variable>
    </function>

	<function name="activeProcessToVar" module="Remote">
        <parameter>MSP</parameter>
        <parameter>msp</parameter>
    </function>

    <function name="executeRemoteXMSCommand" module="Remote">
        <parameter>
            <parameter>run command amocli -k -e (MMS_Req_Message-ID=='</parameter>
            <variable>MessageID</variable>
            <parameter>') search TI:F.CEE.</parameter>
            <variable>msp</variable>
            <parameter>:AMO</parameter>
